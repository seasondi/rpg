<root>
    <Volatile>
        <HasClient>true</HasClient>
        <Persistent>true</Persistent>
        <Router>true</Router>
    </Volatile>

    <Implements>
        <Interface>Inventory</Interface>
    </Implements>

    <Properties>
        <account>
            <Type>STRING</Type>
        </account>
        <level>
            <Type>UINT32</Type>
            <Flags>OWN_CLIENT</Flags>
            <Default>1</Default>
            <Persistent>true</Persistent>
        </level>
        <role_name>
            <Type>STRING</Type>
            <Flags>OWN_CLIENT</Flags>
            <Persistent>true</Persistent>
        </role_name>
        <is_new_role>
            <Type>BOOL</Type>
            <Default>true</Default>
            <Persistent>true</Persistent>
        </is_new_role>
    </Properties>

    <ClientMethods>
        <show_popup_message>
            <Arg> STRING </Arg>
        </show_popup_message>
        <test>
            <t1>ITEM_MAP</t1>
            <t2>ITEM_ARRAY</t2>
            <skill>skill_info</skill>
        </test>
    </ClientMethods>

    <ServerMethods>
        <test> <Exposed/>
            <t1>ITEM_MAP</t1>
            <t2>ITEM_ARRAY</t2>
            <skill>skill_info</skill>
        </test>
    </ServerMethods>
</root>